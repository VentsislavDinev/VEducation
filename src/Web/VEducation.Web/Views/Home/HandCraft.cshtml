@model VEducation.Data.ViewModels.Pages.HomePageViewModel

<section class="hand-crafted bg-light py-5">
    <div class="container py-xl-5 py-lg-3">
        <div class="row banner-grids">
            <div class="col-md-6 banner-image">
                <div class="effect-w3">
                    <img src="images/img.jpg" alt="" class="img-fluid image1">

                </div>

            </div>
            @foreach (var item in Model.About)
            {
                <div class="col-md-6 last-img pl-lg-5 p-3">
                    <h3 class="tittle my-lg-5 my-3"><span class="sub-tittle"> @item.Name </span></h3>
                    @item.Description
                </div>
            }

        </div>
    </div>
</section>
